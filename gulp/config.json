{
    "path": {
        "src": {
            "sass": [
                "src/sass/app/*.scss",
                "src/sass/app/component/**/*.scss"
            ],
            "vendorCss": [
                "node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "assets": "src/assets/"
        },
        "temp": {
            "root": ".tmp/",
            "css": ".tmp/styles/",
            "vendorCss": ".tmp/styles/",
            "assets": ".tmp/assets/"
        },
        "dist": {
            "root": "dist/",
            "css": "dist/styles/",
            "vendorCss": "dist/styles/",
            "assets": "dist/assets/"

        }
    }
}